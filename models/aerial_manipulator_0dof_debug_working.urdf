<?xml version="1.0" ?>
<robot name="AerialManipulator">

 <link name="endeffector"></link>
 <link name="vehicle">
    <inertial>
      <!-- <mass value="1.0"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy=" 0.001" iyz="0.0" izz="0.003"/>
      <origin rpy=" 0 0 0" xyz="0 0.05 0"/> -->
      <mass value="0.83527633"/>
      <inertia ixx="0.003063" ixy="0.000004" ixz="0.000000" iyy="0.001720" iyz="0.000028" izz="0.004585"/>
      <origin rpy="0 0 0" xyz="-1.8626e-09  2.2484e-02 6.7184e-09"/>
      <origin rpy=" 0 0 0" xyz="0 0.05 0"/>
    </inertial>
    <visual>
      <origin xyz=".1 .1 0" rpy="0 0 0" />
      <geometry>
        <cylinder radius=".06477" length="0.007"/>
      </geometry>
      <material name="wash" />
    </visual>
    <visual>
      <origin xyz="-.1 .1 0" rpy="0 0 0" />
      <geometry>
        <cylinder radius=".06477" length="0.007"/>
      </geometry>
      <material name="wash" />
    </visual>
    <visual>
      <origin xyz="-.1 -.1 0" rpy="0 0 0" />
      <geometry>
        <cylinder radius=".06477" length="0.007"/>
      </geometry>
      <material name="wash" />
    </visual>
    <visual>
      <origin xyz=".1 -.1 0" rpy="0 0 0" />
      <geometry>
        <cylinder radius=".06477" length="0.007"/>
      </geometry>
      <material name="wash" />
    </visual>
    <visual>
      <origin xyz="0 .1 0" rpy="1.57068 0 0" />
      <geometry>
        <cylinder radius=".01" length="0.2"/>
      </geometry>
      <material name="wash" />
    </visual>
 </link>

  <joint name="offset" type="fixed" dont_collapse="true">
    <parent link="endeffector"/>
    <child link="vehicle"/>
    <origin rpy="0 0 0" xyz="0 -0.2 0"/>
  </joint>

  <link name="propeller1"></link>
  <link name="propeller2"></link>
  <link name="propeller3"></link>
  <link name="propeller4"></link>

  <joint name="prop1" type="continuous">
    <parent link="vehicle"/>
    <child link="propeller1"/>
    <origin xyz=".1 .1 0" rpy="0 0 0"/>
    <axis rpy="0 0 0" xyz="0 0 1"/>
  </joint>
  <joint name="prop2" type="continuous">
    <parent link="vehicle"/>
    <child link="propeller2"/>
    <origin xyz="-.1 .1 0" rpy="0 0 0"/>
    <axis rpy="0 0 0" xyz="0 0 1"/>
  </joint>
  <joint name="prop3" type="continuous">
    <parent link="vehicle"/>
    <child link="propeller3"/>
    <origin xyz="-.1 -.1 0" rpy="0 0 0"/>
    <axis rpy="0 0 0" xyz="0 0 1"/>
  </joint>
  <joint name="prop4" type="continuous">
    <parent link="vehicle"/>
    <child link="propeller4"/>
    <origin xyz=".1 -.1 0" rpy="0 0 0"/>
    <axis rpy="0 0 0" xyz="0 0 1"/>
  </joint>

</robot>
